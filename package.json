{
  "name": "ics-viewer",
  "version": "0.1.0",
  "private": true,
  "description": "Simple ICS Calendar Viewer",
  "author": "stefanhoelzl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/stefanhoelzl/ics-viewer"
  },
  "scripts": {
    "serve": "vite",
    "build": "vite build"
  },
  "dependencies": {
    "ical.js": "^1.4.0",
    "vue": "^3.2.22",
    "vue-simple-calendar": "^6.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.22",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.2.0",
    "eslint-config-tabsanity": "^2.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.1.1",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "rollup-plugin-typescript2": "^0.31.0",
    "stylelint": "^14.1.0",
    "stylelint-config-standard": "^24.0.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14"
  },
  "eslintConfig": {
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/prettier",
      "tabsanity"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    }
  },
  "prettier": {
    "useTabs": true,
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "printWidth": 180
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "stylelint": {
    "extends": "stylelint-config-standard",
    "exclude": [
      "dist"
    ],
    "rules": {
      "indentation": "tab",
      "declaration-block-trailing-semicolon": null,
      "no-descending-specificity": null
    }
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ]
}
